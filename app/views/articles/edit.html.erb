<%# content_for :admin_pages do %>

<div class="blue">
<div class="main_content">
	
	<h1>Edit Article</h1>	
	
	<% form_for :article, :url => article_url(@article), :html => {:method => :put, :multipart => true} do |f| %>
		<%= f.error_messages %>
		<%= render :partial => 'form', :locals => {:f => f} %>
	<% end %>

</div>	
</div>
<%# end %>